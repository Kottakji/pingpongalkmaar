{
  "private": true,
  "name": "blog",
  "description": "Preact's netlify CMS template",
  "version": "0.1.3",
  "license": "MIT",
  "scripts": {
    "start": "per-env",
    "start:production": "npm run -s serve",
    "start:development": "npm run -s dev",
    "build": "preact build --prerenderUrls ./prerender-urls.js",
    "dev": "preact watch --prerender --prerenderUrls ./prerender-urls.js",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "eslint": "^4.9.0",
    "eslint-config-synacor": "^2.0.2",
    "esm": "^3.2.25",
    "glob": "^7.1.4",
    "markdown": "^0.5.0",
    "node-loader": "^2.0.0",
    "node-sass": "^7.0.1",
    "per-env": "^1.0.2",
    "preact-cli": "^3.4.1",
    "prettier": "^2.7.1",
    "react-syntax-highlighter": "^12.2.1",
    "webpack-bundle-analyzer": "^3.4.1"
  },
  "dependencies": {
    "@babel/preset-react": "^7.18.6",
    "@preact/prerender-data-provider": "^3.0.1",
    "daisyui": "^2.19.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "immer": "^9.0.15",
    "jpegtran-bin": "^7.0.0",
    "markdown-to-jsx": "^6.11.4",
    "moment": "^2.29.4",
    "parse-md": "^2.0.4",
    "preact": "^10.4.6",
    "preact-cli-plugin-netlify": "^1.6.0",
    "preact-render-to-string": "^5.1.10",
    "preact-router": "^3.0.1",
    "sass-loader": "^10.3.1",
    "tailwindcss": "^3.1.6",
    "zustand": "^4.0.0"
  },
  "bugs": {
    "url": "https://github.com/preactjs/preact-netlify/issues"
  },
  "homepage": "https://github.com/preactjs/preact-netlify#readme",
  "keywords": [
    "preact",
    "netlify"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/preactjs/preact-netlify.git"
  }
}
